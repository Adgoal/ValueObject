{
    "name": "adgoal-common/value-object",
    "license": "proprietary",
    "type": "library",
    "description": "A PHP library/collection of classes aimed immutable objects.",
    "require": {
        "php": "^7.3",
        "adgoal-common/base": "^4.0",
        "league/geotools": "^0.8.0",
        "marc-mabe/php-enum": "^4.0",
        "moneyphp/money": "^3.2",
        "ramsey/uuid": "^3.8 || ^4.0",
        "zendframework/zend-validator": "^2.12",
        "beberlei/assert": "^v3.3"
    },
    "require-dev": {
        "jakub-onderka/php-console-highlighter": "^0.4",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "mockery/mockery": "^1.2",
        "phan/phan": "4.0.*",
        "php-coveralls/php-coveralls": "^2.4",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-mockery": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "roave/security-advisories": "dev-master",
        "sensiolabs-de/deptrac-shim": "^0.12.0",
        "symplify/easy-coding-standard": "8.3.* || 9.4.*",
        "vimeo/psalm": "^4.7"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "AdgoalCommon\\ValueObject\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdgoalCommon\\ValueObject\\Tests\\Unit\\": "tests/unit"
        }
    }
}
